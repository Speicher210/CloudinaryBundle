{
  "name": "speicher210/cloudinary-bundle",
  "description": "Bundle for Cloudinary PHP API library",
  "license": "MIT",
  "type": "symfony-bundle",
  "keywords": [
    "cloudinary",
    "symfony",
    "bundle"
  ],
  "authors": [
    {
      "name": "Dragos Protung",
      "email": "dragos@protung.de",
      "role": "Developer"
    }
  ],
  "homepage": "https://github.com/Speicher210/CloudinaryBundle",
  "require": {
    "php": "~8.1.0 || ~8.2.0",
    "cloudinary/cloudinary_php": "^1.20.2",
    "symfony/console": "^5.4 || ^6.0",
    "symfony/finder": "^5.4 || ^6.0",
    "symfony/framework-bundle": "^5.4 || ^6.0",
    "symfony/yaml": "^5.4 || ^6.0"
  },
  "require-dev": {
    "doctrine/coding-standard": "^12.0",
    "ergebnis/composer-normalize": "^2.30.2",
    "phpstan/phpstan": "^1.10.14",
    "phpstan/phpstan-deprecation-rules": "^1.1.3",
    "phpstan/phpstan-phpunit": "^1.3.11",
    "phpstan/phpstan-strict-rules": "^1.5.1",
    "phpstan/phpstan-symfony": "^1.3.1",
    "phpunit/phpunit": "^10.1.2",
    "psalm/plugin-phpunit": "^0.18.4",
    "roave/security-advisories": "dev-latest",
    "thecodingmachine/phpstan-strict-rules": "^1.0.0",
    "twig/twig": "^2.0 || ^3.0",
    "vimeo/psalm": "^5.9.0"
  },
  "suggest": {
    "twig/twig": "Allow to use the cloudinary_url function/filter"
  },
  "minimum-stability": "stable",
  "autoload": {
    "psr-4": {
      "Speicher210\\CloudinaryBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Speicher210\\CloudinaryBundle\\Tests\\": "tests/"
    }
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "ergebnis/composer-normalize": true
    }
  }
}
