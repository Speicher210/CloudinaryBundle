{
  "name": "speicher210/cloudinary-bundle",
  "type": "symfony-bundle",
  "description": "Bundle for Cloudinary PHP API library",
  "keywords": ["cloudinary", "bundle"],
  "homepage": "https://github.com/Speicher210/CloudinaryBundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Dragos Protung",
      "email": "dragos@protung.ro",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^5.6|^7.0",
    "symfony/framework-bundle": "^2.5|^3.0|^4.0",
    "symfony/console": "^2.5|^3.0|^4.0",
    "symfony/finder": "^2.5|^3.0|^4.0",
    "cloudinary/cloudinary_php": "^1.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.7",
    "symfony/phpunit-bridge": "^2.8|^3.0|^4.0",
    "twig/twig": "^1.0|^2.0"
  },
  "suggest": {
    "twig/twig": "Allow to use the cloudinary_url function/filter"
  },
  "autoload": {
    "psr-4": {
      "Speicher210\\CloudinaryBundle\\": ""
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Speicher210\\CloudinaryBundle\\Tests\\": "Tests"
    }
  }
}
