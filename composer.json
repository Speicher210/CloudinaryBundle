{
  "name": "speicher210/cloudinary-bundle",
  "type": "symfony-bundle",
  "description": "Bundle for Cloudinary PHP API library",
  "keywords": ["cloudinary", "bundle"],
  "homepage": "https://github.com/Speicher210/CloudinaryBundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Dragos Protung",
      "email": "dragos@protung.ro",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.1",
    "symfony/framework-bundle": "^3.4|^4.1.12|^5.0",
    "symfony/yaml": "^3.4|^4.1|^5.0",
    "symfony/console": "^3.4|^4.1|^5.0",
    "symfony/finder":"^3.4|^4.1|^5.0",
    "cloudinary/cloudinary_php": "^1.1"
  },
  "require-dev": {
    "roave/security-advisories": "dev-master",
    "symfony/phpunit-bridge": "^3.4|^4.1.12|^5.0",
    "twig/twig": "^1.18|^2.0|^3.0"
  },
  "suggest": {
    "twig/twig": "Allow to use the cloudinary_url function/filter"
  },
  "autoload": {
    "psr-4": {
      "Speicher210\\CloudinaryBundle\\": ""
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Speicher210\\CloudinaryBundle\\Tests\\": "Tests"
    }
  }
}
